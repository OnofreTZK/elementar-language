int mdc(int n, int m){
    int r;
    int mn = m % n
    int nm = n % m

    if (mn =? 0) {
        return r = n;
    } else if (nm =? 0) {
        return r = m
    } else if (m > n) {
        mdc(n, mn, r);
    } else {
        mdc(m, nm, r);
    }
}

int main(){
    int r = mdc(3, 6);
    print(r);
};
